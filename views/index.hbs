<a href="/restaurants/new">New restaurant</a>
<!-- Iterates through each restaurant -->
{{#each restaurants}}
  <div class="restaurant">
    <div class="restaurantName"><a href="/restaurants/{{_id}}">{{name}}</a></div>
    <!-- For each restaurant, it will iterate through their menus -->
    {{#each menus}}
      <p class="menu">{{title}}</p>
    {{/each}}
  </div>
{{/each}}
